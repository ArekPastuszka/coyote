<tr>
    <td>
        <a href="javascript:" class="btn-append" data-url="{{ form.file.value.url }}">{{ form.name.value }}</a>
    </td>
    <td>{{ form.mime.value }}</td>
    <td>{{ form.created_at.value|format_date }}</td>
    <td>{{ (form.size.value / 1024)|round(2) }} kB</td>
    <td>
        <button type="button" title="Usuń załącznik" class="btn btn-default btn-xs btn-del">
            <i class="fa fa-remove"></i>
        </button>

        {{ form_widget(form.id) }}
    </td>
</tr>
