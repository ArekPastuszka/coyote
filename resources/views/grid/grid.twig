<div class="panel panel-default">
    <div class="panel-body">

        {{ pagination|raw }}

        <table class="table table-striped">
            <thead>
                <tr>
                    {% for column in columns %}
                        {{ grid_column(column) }}
                    {% endfor %}
                </tr>
            </thead>
            <tbody>
                {% for row in rows %}
                    <tr>
                        {% for cell in row %}
                            {{ grid_cell(cell) }}
                        {% endfor %}
                    </tr>
                {% endfor %}
            </tbody>
        </table>

        {{ pagination|raw }}
    </div>
</div>

