{
    "name": "adam-boduch/coyote",
    "description": "4programmers.net",
    "version": "2.0-dev",
    "homepage": "http://4programmers.net",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Adam Boduch",
            "email": "adam@boduch.net"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "ext-gd": "*",
        "ext-mongo": "*",
        "laravel/framework": "5.1.*",
        "rcrowe/twigbridge": "0.8.1",
        "laravelcollective/html": "5.1.*",
        "twig/extensions": "^1.3",
        "barryvdh/laravel-debugbar": "^2.0",
        "codeception/codeception": "2.1.4",
        "laravel/socialite": "^2.0",
        "pda/pheanstalk": "^3.1",
        "predis/predis": "^1.0",
        "fzaninotto/faker": "^1.5",
        "erusev/parsedown-extra": "^0.7.0",
        "jenssegers/mongodb": "^2.2",
        "folklore/image": "^0.2.4",
        "ezyang/htmlpurifier": "^4.7",
        "lavary/laravel-menu": "dev-master",
        "elasticsearch/elasticsearch": "~2.0"
    },
    "require-dev": {
        "sebastian/phpcpd": "*",
        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd" : "@stable"
    },
    "autoload": {
        "classmap": [
            "database",
            "app/Libs",
            "app/Models",
            "app/Http/Validators",
            "app/Http/Requests",
            "app/Plugins"
        ],
        "psr-4": {
            "Coyote\\": "app/"
        },
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [

        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
