{
    "name": "adam-boduch/coyote",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/adam-boduch/geshi"
        },
        {
            "type": "vcs",
            "url": "https://github.com/adam-boduch/laravel-grid"
        },
        {
            "type": "vcs",
            "url": "https://github.com/chrisboulton/php-diff"
        }
    ],
    "description": "4programmers.net",
    "version": "2.0-dev",
    "homepage": "http://4programmers.net",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Adam Boduch",
            "email": "adam@boduch.net"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "ext-gd": "*",
        "ext-mongodb": "*",
        "laravel/framework": "5.2.*",
        "rcrowe/twigbridge": "0.9.*",
        "laravelcollective/html": "5.2.*",
        "twig/extensions": "^1.3",
        "barryvdh/laravel-debugbar": "^2.0",
        "laravel/socialite": "^2.0",
        "pda/pheanstalk": "^3.1",
        "predis/predis": "^1.0",
        "erusev/parsedown-extra": "^0.7.0",
        "jenssegers/mongodb": "^3.0",
        "folklore/image": "0.3.*",
        "ezyang/htmlpurifier": "^4.7",
        "lavary/laravel-menu": "dev-master",
        "elasticsearch/elasticsearch": "~2.0",
        "guzzlehttp/guzzle": "^6.2",
        "adam-boduch/geshi": "dev-master",
        "jenssegers/agent": "^2.3",
        "adam-boduch/laravel-grid": "dev-master",
        "chrisboulton/php-diff": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.5",
        "codeception/codeception": "2.2.*",
        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd" : "@stable",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "classmap": [
            "database",
            "app/Models",
            "app/Plugins"
        ],
        "psr-4": {
            "Coyote\\": "app/"
        },
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [

        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.default', '.env');\""
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.production', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
